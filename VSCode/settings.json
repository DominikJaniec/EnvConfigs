// Place your settings in this file to overwrite the default settings
{
    "FSharp.showExplorerOnStartup": false,
    "cSpell.allowCompoundWords": true,
    "cSpell.enableFiletypes": [
        "dockerfile",
        "elm",
        "feature",
        "fsharp",
        "groovy",
        "http",
        "ini",
        "jupyter",
        "powershell",
        "shellscript",
        "sql",
        "sqlite",
        "xml"
    ],
    "diffEditor.renderSideBySide": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.cursorSurroundingLines": 3,
    "editor.gotoLocation.alternativeDefinitionCommand": "editor.action.goToImplementation",
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.parameterHints.cycle": true,
    "editor.snippetSuggestions": "bottom",
    "editor.suggest.localityBonus": true,
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordWrap": "on",
    "explorer.autoReveal": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.codeLens.enabled": false,
    "licenser.author": "Dominik Janiec",
    "licenser.license": "MIT",
    "liveshare.autoShareTerminals": false,
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.integratedConsole.showOnStartup": false,
    "powershell.sideBar.CommandExplorerVisibility": false,
    "scm.alwaysShowRepositories": true,
    "screencastMode.mouseIndicatorSize": 40,
    "search.actionsPosition": "auto",
    "search.collapseResults": "auto",
    "search.searchOnType": false,
    "vim.overrideCopy": false,
    "vim.useCtrlKeys": false,
    "window.restoreWindows": "folders",
    "window.zoomLevel": 1,
    "workbench.quickOpen.closeOnFocusLost": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.restore": false
}
